<html lang="en"><head><meta http-equiv="Content-Type" content="text/html; charset=windows-1252">
    <title>Document</title>
    <link rel="stylesheet" href="./cal_files/style.css">
</head>
<body>
    <div id="calculator" class="cal">
        <h1 id="h1">Calculator</h1>
        <input id="display" readonly="">
        <div id="buttons">
            <button id="1" onclick="appendToDisplay(&#39;/&#39;)" class="opbtn">/</button>
            <button id="2" onclick="appendToDisplay(&#39;1&#39;)">1</button>
            <button id="3" onclick="appendToDisplay(&#39;2&#39;)">2</button>
            <button id="4" onclick="appendToDisplay(&#39;3&#39;)">3</button>
            <button id="5" onclick="appendToDisplay(&#39;*&#39;)" class="opbtn">*</button>
            <button id="6" onclick="appendToDisplay(&#39;4&#39;)">4</button>
            <button id="7" onclick="appendToDisplay(&#39;5&#39;)">5</button>
            <button id="8" onclick="appendToDisplay(&#39;6&#39;)">6</button>
            <button id="9" onclick="appendToDisplay(&#39;+&#39;)" class="opbtn">+</button>
            <button id="0" onclick="appendToDisplay(&#39;7&#39;)">7</button>
            <button id="." onclick="appendToDisplay(&#39;8&#39;)">8</button>
            <button id="+" onclick="appendToDisplay(&#39;9&#39;)">9</button>
            <button id="-" onclick="appendToDisplay(&#39;-&#39;)" class="opbtn">-</button>
            <button id="*" onclick="appendToDisplay(&#39;0&#39;)">0</button>
            <button id="/" onclick="appendToDisplay(&#39;.&#39;)">.</button>
            <button id="=" onclick="calculate()" class="sum">=</button>
            <button id="C" onclick="clearDisplay()" class="clear">C</button>

        </div>
    </div>
    <script src="./cal_files/script.js.download"></script>

</body></html>